{
    "name": "doctrine/mongodb-odm-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Doctrine MongoDB Bundle",
    "keywords": ["persistence", "mongodb", "symfony"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {"name": "Bulat Shakirzyanov", "email": "mallluhuct@gmail.com"},
        {"name": "Kris Wallsmith", "email": "kris@symfony.com"},
        {"name": "Jonathan H. Wage", "email": "jonwage@gmail.com"}
    ],
    "require": {
        "php": "^7.1",
        "doctrine/mongodb-odm": "^2.0@dev",
        "psr/log": "^1.0",
        "symfony/options-resolver": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/console": "^4.0",
        "symfony/config": "^4.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "@dev",
        "symfony/form": "^4.0",
        "symfony/yaml": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "suggest": {
        "doctrine/data-fixtures": "Load data fixtures"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\Bundle\\MongoDBBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
